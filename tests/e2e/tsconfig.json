{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "/presupplied/*": ["../../*"]
    },
    "types": ["bun-types"],
    "lib": ["ESNext", "DOM", "DOM.Iterable"]
  },
  "include": ["**/*.ts"]
}