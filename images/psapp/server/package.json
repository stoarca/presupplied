{
  "name": "presupplied-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "migration:generate": "cd src/migration && typeorm-ts-node-commonjs migration:generate -d ../data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts --transaction each",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts --transaction each",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/data-source.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-http-proxy": "^1.6.3",
    "jiti": "^1.20.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "tsx": "^3.13.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.17",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^18.11.18",
    "css-loader": "^6.7.3",
    "nodemon": "^3.0.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
